{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/jsonrpc-generator/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/jsonrpc-generator",
        "tsConfig": "libs/jsonrpc-generator/tsconfig.lib.json",
        "project": "libs/jsonrpc-generator/package.json",
        "entryFile": "libs/jsonrpc-generator/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/jsonrpc-generator/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/jsonrpc-generator/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/jsonrpc-generator"],
      "options": {
        "jestConfig": "libs/jsonrpc-generator/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build-spec": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "yarn tsc --project ./libs/jsonrpc-generator/tsconfig.spec.json"
      }
    }
  }
}
